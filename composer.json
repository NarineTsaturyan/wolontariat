{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/a11y_paragraphs_tabs": "^1.11",
        "drupal/admin_toolbar": "^3.1",
        "drupal/antibot": "^2.0",
        "drupal/asset_injector": "^2.10",
        "drupal/authorization": "^1.0@beta",
        "drupal/better_exposed_filters": "^5.2",
        "drupal/block_class": "^1.3",
        "drupal/block_content_permissions": "^1.10",
        "drupal/block_permissions": "^1.2",
        "drupal/body_node_id_class": "^1.1",
        "drupal/bootstrap": "^3.24",
        "drupal/bootstrap_layouts": "^5.2",
        "drupal/calendar_link": "^2.1",
        "drupal/captcha": "^1.2",
        "drupal/checklistapi": "^2.0",
        "drupal/chosen": "3.0",
        "drupal/ckeditor_accordion": "^1.3",
        "drupal/ckeditor_font": "^1.2",
        "drupal/ckeditor_youtube": "^1.4",
        "drupal/colorbox": "^2.0",
        "drupal/colorbutton": "^1.3",
        "drupal/consent_manager": "2.0",
        "drupal/console": "^1.9",
        "drupal/context": "^4.1",
        "drupal/cookie_content_blocker": "^2.0",
        "drupal/cookies": "^1.0",
        "drupal/core": "^9.3",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-project-message": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/create_user_permission": "^2.0",
        "drupal/ctools": "^3.7",
        "drupal/devel": "^5.0",
        "drupal/ds": "^3.13",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/editor_advanced_link": "^2.0",
        "drupal/editor_file": "^1.6",
        "drupal/email_blocker": "^1.3",
        "drupal/entity": "^1.3",
        "drupal/entity_class_formatter": "^1.3",
        "drupal/entity_print": "^2.4",
        "drupal/entity_reference_revisions": "^1.9",
        "drupal/eu_cookie_compliance": "^1.19",
        "drupal/externalauth": "^1.0",
        "drupal/feeds": "^3.0@beta",
        "drupal/field_group": "^3.2",
        "drupal/field_token_value": "^2.0",
        "drupal/filebrowser": "^3.0",
        "drupal/flexslider": "^2.0",
        "drupal/fontawesome": "^2.22",
        "drupal/form_mode_control": "^2.1",
        "drupal/form_mode_manager": "^2.0@RC",
        "drupal/form_protect": "^2.0",
        "drupal/forms_steps": "^1.5",
        "drupal/google_analytics": "^4.0",
        "drupal/honeypot": "^2.0",
        "drupal/image_effects": "^3.2",
        "drupal/image_widget_crop": "^2.3",
        "drupal/imagefield_tokens": "^2.23",
        "drupal/imce": "^2.4",
        "drupal/jquery_ui_tooltip": "^1.1",
        "drupal/ldap": "^4.3",
        "drupal/link_attributes": "^1.11",
        "drupal/linked_field": "^1.3",
        "drupal/mail_login": "^2.6",
        "drupal/mailsystem": "^4.3",
        "drupal/matomo": "^1.17",
        "drupal/menu_block": "^1.8",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/menu_manipulator": "^3.0",
        "drupal/menu_per_role": "^1.4",
        "drupal/metatag": "^1.19",
        "drupal/migrate_plus": "^5.2",
        "drupal/migrate_tools": "^5.1",
        "drupal/miniorange_saml": "^2.25",
        "drupal/module_filter": "^3.2",
        "drupal/node_title_help_text": "^1.2",
        "drupal/node_view_permissions": "^1.5",
        "drupal/noreqnewpass": "^1.2",
        "drupal/page_title_visibility": "1.4",
        "drupal/paragraphs": "^1.13",
        "drupal/pathauto": "^1.9",
        "drupal/podcast": "^1.2",
        "drupal/prlp": "^1.7",
        "drupal/quick_node_clone": "^1.14",
        "drupal/rebuild_cache_access": "^1.8",
        "drupal/recaptcha_v3": "^1.4",
        "drupal/redirect": "^1.7",
        "drupal/redirect_after_login": "^2.7",
        "drupal/redis": "^1.5",
        "drupal/responsive_menu": "^4.4",
        "drupal/searchindex_wipe": "^1.7",
        "drupal/semanticviews": "^2.3",
        "drupal/simple_menu_icons": "^2.3",
        "drupal/simple_sitemap": "^4.1",
        "drupal/smart_date": "^3.5",
        "drupal/smart_trim": "^1.3",
        "drupal/smtp": "^1.0",
        "drupal/superfish": "^1.4",
        "drupal/swiftmailer": "^2.2",
        "drupal/taxonomy_import": "^2.0",
        "drupal/taxonomy_menu": "^3.5",
        "drupal/text_resize": "^1.2",
        "drupal/token": "^1.10",
        "drupal/translatable_menu_link_uri": "^2.0",
        "drupal/unique_field_ajax": "^2.1",
        "drupal/upgrade_status": "^3.14",
        "drupal/userprotect": "^1.1",
        "drupal/vbo_export": "^4.0",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_bootstrap": "^4.3",
        "drupal/views_bulk_operations": "^4.1",
        "drupal/views_conditional": "^1.4",
        "drupal/views_fieldsets": "^3.4",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/viewsreference": "^1.7",
        "drupal/webform": "^6.1",
        "drupal/workbench_email": "^2.2",
        "drupal/youtube": "^1.2",
        "drush/drush": "^11.0",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/webform/composer.libraries.json"
            ]
        }
    }
}
